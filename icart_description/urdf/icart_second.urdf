<?xml version="1.0"?>
<robot name="icart_second">
  <!--visual&collision define-->
  <!--icart_body-->
  <link name="base_link_second">
    <visual>
      <geometry>
        <mesh filename="package://icart_description/mesh/DAE/i-Cart_body.dae"/>
      </geometry>
    </visual>

    <collision>
      <geometry>
        <mesh filename="package://icart_description/mesh/DAE/i-Cart_body.dae"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.000"/>
      <!--<inertia ixx="0.0020833333" ixy="0" ixz="0" iyy="0.0070833333" iyz="0" izz="0.0083333333"/>-->
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>

  <!--icart_caster-->
  <link name="caster_link_second">
    <visual>
      <geometry>
        <!--<mesh filename="package://icart_description/mesh/DAE/i-Cart_caster.dae"/>-->
        <sphere radius="0.0325"/>
      </geometry>
    </visual>

    <collision>
      <geometry>
        <mesh filename="package://icart_description/mesh/DAE/i-Cart_caster.dae"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.500"/>
      <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025"  iyz="0" izz="0.00025"/>
     </inertial>
  </link>

  <!--icart_right_wheel-->
  <link name="right_wheel_link_second">
    <visual>
      <geometry>
        <!--<mesh filename="package://icart_description/mesh/DAE/i-Cart_right_wheel.dae"/>-->
        <cylinder radius="0.0325" length="0.01"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="black"/>
    </visual>

    <collision>
      <geometry>
        <!--<mesh filename="package://icart_description/mesh/DAE/i-Cart_right_wheel.dae"/>-->
        <cylinder radius="0.0325" length="0.01"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="1.500"/>
      <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025"/>
   </inertial>
  </link>

  <!--icart_left_wheel-->
  <link name="left_wheel_link_second">
    <visual>
      <geometry>
        <!--<mesh filename="package://icart_description/mesh/DAE/i-Cart_left_wheel.dae"/>-->
        <cylinder radius="0.0325" length="0.01"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="black"/>
    </visual>

    <collision>
      <geometry>
        <!--<mesh filename="package://icart_description/mesh/DAE/i-Cart_left_wheel.dae"/>-->
        <cylinder radius="0.0325" length="0.01"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="1.500"/>
      <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025"/>
   </inertial>
  </link>

  <!-- Hokuyo Laser -->
  <link name="hokuyo_link_second">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://rrbot_description/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>


  <!--joint define-->
  <!--base_link to caster-->
  <joint name="caster_joint_second" type="fixed">
    <parent link="base_link_second"/>
    <child link="caster_link_second"/>
    <origin xyz="-0.096 0 -0.019" rpy="0 0 0"/>
  </joint>

  <!--base_link to left_wheel-->
  <joint name="left_wheel_joint_second" type="continuous">
    <parent link="base_link_second"/>
    <child link="left_wheel_link_second"/>
    <origin xyz="0.105 0.178 -0.022" rpy="-1.5707 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!--base_link to right_wheel-->
  <joint name="right_wheel_joint_second" type="continuous">
    <parent link="base_link_second"/>
    <child link="right_wheel_link_second"/>
    <origin xyz="0.105 -0.178 -0.022" rpy="1.5707 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <!--base_link to hokuyo-->
  <joint name="hokuyo_joint_second" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0.105 0 0.035" rpy="0 0 0"/>
    <parent link="base_link_second"/>
    <child link="hokuyo_link_second"/>
  </joint>

  <!--define base_foot-->
  <link name="base_footprint_second"/>
    <joint name="base_link_joint_second" type="fixed">
    <parent link="base_footprint_second"/>
    <child link="base_link_second"/>
    <origin xyz="-0.105 0 0.0545"/>
  </joint>

  <!-- ===============  Transmission =============== -->

  <transmission name="left_wheel_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="left_wheel_joint_second">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
      </joint>
   <actuator name="left_wheel_motor">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
  <mechanicalReduction>30</mechanicalReduction>
      </actuator>
 </transmission>

  <transmission name="right_wheel_trans">
       <type>transmission_interface/SimpleTransmission</type>
       <joint name="right_wheel_joint_second">
   <hardwareInterface>VelocityJointInterface</hardwareInterface>
       </joint>
    <actuator name="right_wheel_motor">
   <hardwareInterface>VelocityJointInterface</hardwareInterface>
   <mechanicalReduction>30</mechanicalReduction>
       </actuator>
  </transmission>



 <!-- =============== Gazebo =============== -->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>icart_second</robotNamespace>
      <robotParam>robot_description_second</robotParam>
    </plugin>
  </gazebo>

 <gazebo reference="base_link_second">
   <selfCollide>true</selfCollide>
    <mu1 value="0.05" />
    <mu2 value="0.05" />
  </gazebo>

 <gazebo reference="left_wheel_link_second">
   <selfCollide>false</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

 <gazebo reference="right_wheel_link_second">
   <selfCollide>false</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

 <gazebo reference="caster_link_second">
   <selfCollide>true</selfCollide>
     <!-- This caster is no frictional resistance. -->
    <mu1 value="0.0" />
    <mu2 value="0.0" />
  </gazebo>

  <!-- hokuyo -->
  <gazebo reference="hokuyo_link_second">
    <sensor type="ray" name="head_hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-1.570796</min_angle>
            <max_angle>1.570796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>2.00</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/icart_second/laser/scan</topicName>
        <frameName>hokuyo_link_second</frameName>
      </plugin>
    </sensor>
  </gazebo>
</robot>
