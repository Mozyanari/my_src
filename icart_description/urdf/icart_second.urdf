  <!--joint define-->
  <!--base_link to base_foot_print-->
  <joint name="foot_print_second" type="fixed">
    <parent link="base_foot_print_second"/>
    <child link="base_link_second"/>
    <origin xyz="-0.105 0 ${cr}" rpy="0 0 0"/>
  </joint>

  <!--base_link to base_link_offset-->
  <joint name="offset_second" type="fixed">
    <parent link="base_link_second"/>
    <child link="base_link_second_offset"/>
    <origin xyz="-0.055 0 0" rpy="0 0 0"/>
  </joint>

  <!--base_link to caster-->
  <joint name="caster_joint_second" type="fixed">
    <parent link="base_link_second"/>
    <child link="caster_link_second"/>
    <origin xyz="-0.096 0 0" rpy="0 0 0"/>
  </joint>

  <!--base_link to left_wheel-->
  <joint name="left_wheel_joint_second" type="continuous">
    <parent link="base_link_second"/>
    <child link="left_wheel_link_second"/>
    <origin xyz="0.105 0.178 0" rpy="-1.5707 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!--base_link to right_wheel-->
  <joint name="right_wheel_joint_second" type="continuous">
    <parent link="base_link_second"/>
    <child link="right_wheel_link_second"/>
    <origin xyz="0.105 -0.178 0" rpy="1.5707 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <!--base_link to hokuyo-->
  <joint name="hokuyo_joint_second" type="fixed">
    <axis xyz="0 0 0" />
    <origin xyz="0 0 0.0825" rpy="0 0 3.141592"/>
    <parent link="base_foot_print_second"/>
    <child link="hokuyo_link_second"/>
  </joint>

  <!--base_link_offset to stick-->
  <joint name="stick_second" type="fixed">
    <parent link="base_link_second_offset"/>
    <child link="stick_second"/>
    <origin xyz="0 0 ${sh/2+bh/2}" rpy="0 0 0"/>
  </joint>

  <!--stick to table-->
  <joint name="table_joint_second" type="continuous">
    <parent link="stick_second"/>
    <child link="table_second"/>
    <origin xyz="0 0 ${tl/2}" rpy="0 0 0"/>
    <axis xyz="0 0 1" />
  </joint>

