  <!-- ===============  Transmission =============== -->

  <transmission name="left_wheel_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="left_wheel_joint_second">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
      </joint>
   <actuator name="left_wheel_motor">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
  <mechanicalReduction>30</mechanicalReduction>
      </actuator>
 </transmission>

  <transmission name="right_wheel_trans">
       <type>transmission_interface/SimpleTransmission</type>
       <joint name="right_wheel_joint_second">
   <hardwareInterface>VelocityJointInterface</hardwareInterface>
       </joint>
    <actuator name="right_wheel_motor">
   <hardwareInterface>VelocityJointInterface</hardwareInterface>
   <mechanicalReduction>30</mechanicalReduction>
       </actuator>
  </transmission>



 <!-- =============== Gazebo =============== -->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>icart_second</robotNamespace>
      <robotParam>robot_description_second</robotParam>
    </plugin>
  </gazebo>

 <gazebo reference="base_link_second">
   <selfCollide>true</selfCollide>
    <mu1 value="0.05" />
    <mu2 value="0.05" />
  </gazebo>

 <gazebo reference="left_wheel_link_second">
   <selfCollide>true</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

 <gazebo reference="right_wheel_link_second">
   <selfCollide>true</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

 <gazebo reference="caster_link_second">
   <selfCollide>true</selfCollide>
     <!-- This caster is no frictional resistance. -->
    <mu1 value="0.0" />
    <mu2 value="0.0" />
  </gazebo>

  <gazebo reference="stick_second">
   <selfCollide>true</selfCollide>
     <!-- This caster is no frictional resistance. -->
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

  <gazebo reference="table_second">
   <selfCollide>true</selfCollide>
     <!-- This caster is no frictional resistance. -->
    <mu value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

  <gazebo reference="stopper1_second">
   <selfCollide>true</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

  <gazebo reference="stopper2_second">
   <selfCollide>true</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

  <gazebo reference="stopper3_second">
   <selfCollide>true</selfCollide>
    <mu1 value="1.0" />
    <mu2 value="1.0" />
  </gazebo>

